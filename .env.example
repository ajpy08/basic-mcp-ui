# Configuración del Servidor MCP
HOST_MCP=127.0.0.1
PORT_MCP=8000

# Configuración del Servidor Web UI
HOST_UI=127.0.0.1
PORT_UI=8001

# Configuración de Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(levelname)s | %(name)s | %(message)s

# Configuración de la Aplicación
APP_NAME=Calculator Server
